{
  "private": true,
  "name": "septagon-repository",
  "version": "0.0.1",
  "description": "npm repository manager and proxy",
  "author": "Christian Rudh <christian@rudh.se>",
  "license": "UNLICENSED",
  "main": "server/server_.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server_.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server server/server_.js",
    "dev:client": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js",
    "lint": "eslint server test",
    "test": "npm-run-all test:clean test:server",
    "test:server": "cross-env CONFIG=test mocha --compilers js:babel-core/register --recursive server/test/*.test.js --timeout 10000",
    "test:clean": "rimraf tmp/testRepository"
  },
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "body-parser": "1.15.2",
    "cross-env": "3.0.0",
    "event-stream": "3.3.4",
    "express": "4.14.0",
    "mkdirp": "0.5.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "request": "2.75.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "chai": "3.5.0",
    "eslint": "3.7.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "mocha": "3.1.0",
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "rimraf": "2.5.4",
    "supertest": "2.0.0",
    "webpack": "1.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crudh/septagon-repository.git"
  },
  "bugs": {
    "url": "https://github.com/crudh/septagon-repository/issues"
  },
  "homepage": "https://github.com/crudh/septagon-repository#readme",
  "keywords": [
    "npm",
    "repository",
    "manager",
    "registry",
    "proxy"
  ]
}
