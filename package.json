{
  "private": true,
  "name": "septagon-repository",
  "version": "0.0.1",
  "description": "npm repository manager and proxy",
  "author": "Christian Rudh <christian@rudh.se>",
  "license": "UNLICENSED",
  "main": "server/server_.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server_.js",
    "build": "npm-run-all build:clean build:client",
    "build:client": "cross-env NODE_ENV=production webpack -p",
    "build:clean": "rimraf ./public/build",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch server server/server_.js",
    "dev:client": "cross-env NODE_ENV=development webpack --watch",
    "lint": "eslint server client",
    "test": "npm-run-all test:clean test:server",
    "test:server": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive server/test/*.test.js --timeout 10000",
    "test:clean": "rimraf ./tmp/testRepository"
  },
  "dependencies": {
    "babel-core": "6.21.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "body-parser": "1.15.2",
    "config": "1.24.0",
    "cross-env": "3.1.4",
    "event-stream": "3.3.4",
    "express": "4.14.0",
    "lodash": "4.17.4",
    "mkdirp": "0.5.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "request": "2.79.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "chai": "3.5.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "html-webpack-plugin": "2.26.0",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.0",
    "rimraf": "2.5.4",
    "supertest": "2.0.1",
    "webpack": "2.2.0-rc.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crudh/septagon-repository.git"
  },
  "bugs": {
    "url": "https://github.com/crudh/septagon-repository/issues"
  },
  "homepage": "https://github.com/crudh/septagon-repository#readme",
  "keywords": [
    "npm",
    "repository",
    "manager",
    "registry",
    "proxy"
  ]
}
