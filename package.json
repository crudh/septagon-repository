{
  "private": true,
  "name": "septagon-repository",
  "version": "0.0.1",
  "description": "npm repository manager and proxy",
  "author": "Christian Rudh <christian@rudh.se>",
  "license": "UNLICENSED",
  "main": "server/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch server server/server.js",
    "lint": "eslint server",
    "lint:fix": "eslint --fix server",
    "test": "npm-run-all test:clean test:server",
    "test:server": "cross-env NODE_ENV=test jest",
    "test:clean": "rimraf ./tmp/testRepository"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "config": "1.26.2",
    "cross-env": "5.0.5",
    "event-stream": "3.3.4",
    "express": "4.16.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "mkdirp": "0.5.1",
    "request": "2.83.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "eslint": "4.8.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-prettier": "2.3.1",
    "jest": "21.2.1",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "prettier": "1.7.4",
    "rimraf": "2.6.2",
    "supertest": "3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crudh/septagon-repository.git"
  },
  "bugs": {
    "url": "https://github.com/crudh/septagon-repository/issues"
  },
  "homepage": "https://github.com/crudh/septagon-repository#readme",
  "keywords": [
    "npm",
    "repository",
    "manager",
    "registry",
    "proxy"
  ]
}
