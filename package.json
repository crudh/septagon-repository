{
  "private": true,
  "name": "septagon-repository",
  "version": "0.0.1",
  "description": "npm repository manager and proxy",
  "author": "Christian Rudh <christian@rudh.se>",
  "license": "UNLICENSED",
  "main": "server/server_.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server_.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch server server/server_.js",
    "lint": "eslint server test",
    "test": "cross-env CONFIG=test mocha --compilers js:babel-core/register --recursive server/test/test*.js --timeout 10000"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "body-parser": "^1.15.0",
    "config": "^1.20.1",
    "cross-env": "^1.0.7",
    "event-stream": "^3.3.2",
    "express": "^4.13.4",
    "mkdirp": "^0.5.1",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "supertest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crudh/septagon-repository.git"
  },
  "bugs": {
    "url": "https://github.com/crudh/septagon-repository/issues"
  },
  "homepage": "https://github.com/crudh/septagon-repository#readme",
  "keywords": [
    "npm",
    "repository",
    "manager",
    "registry",
    "proxy"
  ]
