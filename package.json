{
  "private": true,
  "name": "septagon-repository",
  "version": "0.0.1",
  "description": "npm repository manager and proxy",
  "author": "Christian Rudh <christian@rudh.se>",
  "license": "UNLICENSED",
  "main": "server/server_.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server_.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    // fix npm watch and stuff
    "dev": "cross-env NODE_ENV=development nodemon --watch server server/server_.js",
    "lint": "eslint server test",
    "test": "npm-run-all test:clean test:server",
    "test:server": "cross-env CONFIG=test mocha --compilers js:babel-core/register --recursive server/test/test*.js --timeout 10000",
    "test:clean": "rimraf tmp/testRepository"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "body-parser": "^1.15.0",
    "config": "^1.20.1",
    "cross-env": "^1.0.7",
    "event-stream": "^3.3.2",
    "express": "^4.13.4",
    "mkdirp": "^0.5.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.0.0",
    "redux-thunk": "^2.0.1",
    "request": "^2.69.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.8.0",
    "rimraf": "^2.5.2",
    "supertest": "^1.2.0",
    "webpack": "^1.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crudh/septagon-repository.git"
  },
  "bugs": {
    "url": "https://github.com/crudh/septagon-repository/issues"
  },
  "homepage": "https://github.com/crudh/septagon-repository#readme",
  "keywords": [
    "npm",
    "repository",
    "manager",
    "registry",
    "proxy"
  ]
}
