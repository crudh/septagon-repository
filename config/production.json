{
  "server": {
    "location": {
      "protocol": "http",
      "host": "localhost",
      "port": "3000"
    },
    "repos": {
      "main": {
        "id": "main",
        "upstream": "https://registry.npmjs.org",
        "storage": "./repositories/main",
        "public": false
      }
    },
    "log": {
      "file": {
        "filename": "server.log",
        "level": "info",
        "handleExceptions": true,
        "humanReadableUnhandledException": true
      },
      "console": {
        "level": "error"
      }
    }
  }
}
